{
  "name": "swarmblitz-io",
  "version": "2.0.0",
  "private": true,
  "description": "SwarmBlitz.io - A multiplayer territory capture game with free movement (Cloudflare Workers Edition)",
  "main": "workers/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:worker\" \"npm run dev:client\"",
    "dev:metrics": "concurrently \"npm run dev:worker:metrics\" \"npm run dev:client\"",
    "dev:worker": "wrangler dev",
    "dev:worker:metrics": "wrangler dev --var METRICS_ENABLED:true",
    "dev:client": "vite",
    "build": "vite build",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "load:test": "node tools/load-test.js",
    "load:test:50": "node tools/load-test.js 50 ws://localhost:8787/room/default 30000",
    "load:test:100": "node tools/load-test.js 100 ws://localhost:8787/room/default 60000",
    "deprecated:start": "node deprecated/server.js",
    "deprecated:dev": "nodemon --watch deprecated/ deprecated/server.js"
  },
  "repository": "stevenjoezhang/paper.io",
  "keywords": [
    "Paper-IO",
    "IO",
    "Game",
    "Cloudflare Workers",
    "Durable Objects"
  ],
  "author": "Mimi <stevenjoezhang@gmail.com> (https://zhangshuqiao.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevenjoezhang/paper.io/issues"
  },
  "homepage": "https://github.com/stevenjoezhang/paper.io",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "polygon-clipping": "^0.15.7"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "vite": "^6.4.1",
    "wrangler": "^3.0.0",
    "ws": "^8.19.0"
  }
}
